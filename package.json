{
  "name": "san-queries",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "private": true,
  "engines": {
    "node": "<=14"
  },
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --write"
    ]
  },
  "precommit": [
    "test"
  ],
  "prepush": [
    "test"
  ],
  "scripts": {
    "dev": "vite --config vite/config.js",
    "build": "npm run webkit && npm run get-schema && vite build --config vite/config.js",
    "serve": "vite preview",
    "test": "jest",
    "validate": "svelte-check",
    "validate:errors": "yarn validate --threshold error",
    "lib:prepare": "node scripts/prepare.js && tsc --p ./scripts/tsconfig.json && node scripts/cleanup.js",
    "lib": "npm run lib:prepare && node scripts/lib.js",
    "lib:publish": "node scripts/publish",
    "webkit": "node scripts/webkit",
    "prepare": "husky install",
    "postinstall": "patch-package && npm run webkit && npm run get-schema",
    "get-schema": "babel-node --presets @babel/preset-env -o scripts,node_modules/san-webkit scripts/schema.js"
  },
  "dependencies": {
    "marked": "^4.0.18",
    "medium-editor": "^5.23.3",
    "monaco-editor": "^0.33.0",
    "monaco-clickhouse": "https://github.com/DmitriVanGuard/monaco-editor#92558250",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9"
  },
  "devDependencies": {
    "san-studio": "https://github.com/santiment/san-studio#1fb1fc7",
    "san-webkit": "https://github.com/santiment/san-webkit#b57c553",
    "@santiment-network/chart": "^0.10.12",
    "sass": "^1.49.8",
    "turndown": "^7.1.1",
    "monaco-editor-core": "0.33.0-dev.20220214",
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-svelte3-preprocess": "0.0.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^4.7.4",
    "svelte-preprocess-cssmodules": "https://github.com/DmitriVanGuard/svelte-preprocess-cssmodules#3b24f22",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-ts-checker": "^0.1.5",
    "mathjs": "^9.4.4"
  },
  "peerDependencies": {
    "san-webkit": "https://github.com/santiment/san-webkit",
    "san-studio": "https://github.com/santiment/san-studio",
    "monaco-clickhouse": "https://github.com/microsoft/monaco-editor#305b9db"
  }
}
