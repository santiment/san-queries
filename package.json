{
  "name": "san-queries",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "npm run webkit && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 4000 --ci",
    "build-storybook": "storybook build",
    "webkit": "node scripts/webkit",
    "prepare": "npm run webkit",
    "patch:svelte-kit": "node scripts/patch"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^1.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/blocks": "^7.0.26",
    "@storybook/svelte": "^7.0.26",
    "@storybook/sveltekit": "^7.0.26",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-svelte": "^2.32.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "san-webkit": "https://github.com/santiment/san-webkit#a4779ef",
    "sass": "^1.63.6",
    "storybook": "^7.0.26",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.5",
    "svelte-preprocess": "^5.0.4",
    "svelte-preprocess-style-child-component": "^0.1.7",
    "svelte-preprocess-cssmodules": "https://github.com/DmitriVanGuard/svelte-preprocess-cssmodules#3b24f22",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "pnpm": {
    "patchedDependencies": {
      "@sveltejs/kit@1.22.1": "patches/@sveltejs__kit@1.22.1.patch",
      "svelte-preprocess-style-child-component@0.1.7": "patches/svelte-preprocess-style-child-component@0.1.7.patch"
    }
  }
}
