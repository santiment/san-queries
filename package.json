{
  "name": "san-queries",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "private": true,
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --plugin-search-dir . --write"
    ],
    "*.{js,ts,svelte}": [
      "eslint --fix",
      "npm run check"
    ]
  },
  "scripts": {
    "dev": "vite dev",
    "build": "npm run webkit && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "storybook dev -p 4000 --ci",
    "build-storybook": "storybook build",
    "webkit": "node scripts/webkit",
    "prepare": "husky install && npm run webkit",
    "patch:svelte-kit": "node scripts/patch",
    "get-schema": "node scripts/schema.js --run"
  },
  "dependencies": {
    "@popsql/monaco-sql-languages": "^0.0.3",
    "@sveltejs/adapter-node": "^1.3.1",
    "marked": "^6.0.0",
    "medium-editor": "^5.23.3",
    "monaco-editor": "^0.41.0",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@storybook/addon-essentials": "^7.2.2",
    "@storybook/addon-interactions": "^7.2.2",
    "@storybook/addon-links": "^7.2.2",
    "@storybook/blocks": "^7.1.0",
    "@storybook/svelte": "^7.2.2",
    "@storybook/sveltekit": "^7.2.2",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/kit": "^1.22.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte": "^2.32.4",
    "graphql": "^16.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "miragejs": "^0.1.47",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "san-webkit": "https://github.com/santiment/san-webkit#2cda5e8",
    "sass": "^1.65.1",
    "storybook": "^7.2.2",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "svelte-preprocess-cssmodules": "https://github.com/DmitriVanGuard/svelte-preprocess-cssmodules#3b24f22",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "pnpm": {
    "patchedDependencies": {}
  },
  "msw": {
    "workerDirectory": ".storybook"
  }
}
