{
  "name": "san-queries",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "private": true,
  "engines": {
    "node": "<=14"
  },
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --write"
    ]
  },
  "precommit": [
    "test"
  ],
  "prepush": [
    "test"
  ],
  "scripts": {
    "dev": "vite --config vite/config.js",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest",
    "validate": "svelte-check",
    "validate:errors": "yarn validate --threshold error",
    "lib:prepare": "node scripts/prepare.js && tsc --p ./scripts/tsconfig.json && node scripts/cleanup.js",
    "lib": "npm run lib:prepare && node scripts/lib.js",
    "-install": "npm run lib",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-svelte3-preprocess": "0.0.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "vite": "^2.9.13",
    "vite-plugin-ts-checker": "^0.1.5"
  },
  "dependencies": {
    "san-webkit": "https://github.com/santiment/san-webkit#ef0819e",
    "svelte-preprocess": "^4.7.4",
    "svelte-preprocess-cssmodules": "https://github.com/DmitriVanGuard/svelte-preprocess-cssmodules#3b24f22",
    "typescript": "^4.7.4",
    "phoenix": "^1.6.6"
  },
  "peerDependencies": {
    "san-webkit": "https://github.com/santiment/san-webkit.git"
  }
}
