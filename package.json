{
  "name": "san-queries",
  "version": "0.0.1",
  "author": "DmitriVanGuard <dimavanguard@gmail.com>",
  "type": "module",
  "private": true,
  "lint-staged": {
    "*.{js,ts,svelte,css,md,mdx}": [
      "prettier --plugin-search-dir . --write"
    ],
    "*.{js,ts,svelte}": [
      "eslint --fix",
      "npm run check"
    ]
  },
  "scripts": {
    "dev": "vite dev",
    "build": "npm run webkit && npm run get-schema && vite build",
    "get-schema": "node scripts/schema.js --run",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storybook": "npm run webkit && storybook dev -p 4000 --ci --disable-telemetry",
    "build-storybook": "storybook build",
    "webkit": "node scripts/webkit"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.18",
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.76.0",
    "@playwright/test": "^1.42.0",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-themes": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/svelte": "^8.0.8",
    "@storybook/sveltekit": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/eslint": "^8.56.5",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "2.36.0-next.13",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.8",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "sass": "^1.72.0",
    "storybook": "^8.0.8",
    "svelte": "5.0.0-next.113",
    "svelte-check": "^3.6.9",
    "svelte-preprocess-cssmodules": "github:DmitriVanGuard/svelte-preprocess-cssmodules#3b24f22",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@popsql/monaco-sql-languages": "^0.1.0",
    "@sveltejs/adapter-node": "^5.0.1",
    "@xyflow/svelte": "^0.0.41",
    "chart.js": "^4.4.2",
    "esm-env": "^1.0.0",
    "monaco-editor": "^0.47.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rxjs": "^7.8.1",
    "san-webkit": "github:santiment/san-webkit#15afa8a",
    "svelte-runes": "^0.0.3",
    "tailwind-merge": "^2.2.1",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "@sveltejs/kit@2.5.6": "patches/@sveltejs__kit@2.5.6.patch"
    }
  }
}
