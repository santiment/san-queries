{
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "trailingComma": "all",
  "semi": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "endOfLine": "lf",
  "printWidth": 100,
  "singleQuote": true
}
